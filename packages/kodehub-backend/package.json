{
  "name": "gql-express",
  "version": "1.0.0",
  "description": "",
  "main": "src/app/server.js",
  "engines": {
    "node": ">=10.0.0 <=12.16.1"
  },
  "scripts": {
    "dev": "nodemon --watch src --exec babel-node src/app/server.js",
    "start": "node dist/app/server.js",
    "start:prod": "cross-env NODE_ENV=production node dist/app/server.js",
    "test": "jest --runInBand --coverage --verbose",
    "start:test": "cross-env NODE_ENV=test MONGODB_DEBUG=true npm run test:watch",
    "lint": "eslint '**/*.js' --ignore-pattern node_modules/",
    "fix:lint": "eslint '**/*.js' --ignore-pattern node_modules/ --fix",
    "seed": "babel-node seeds/index.js",
    "build": "cross-env NODE_ENV=production babel src -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.10.1",
    "apollo-server-express": "^2.10.1",
    "bcrypt": "^3.0.8",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "graphql": "^14.6.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-scalars": "^1.0.6",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.1",
    "morgan": "^1.9.1",
    "password-validator": "^5.0.3",
    "remove": "^0.1.5",
    "uuid": "^3.4.0",
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jest-runner-eslint": "^0.7.6",
    "mongodb-memory-server": "^6.3.2",
    "nodemon": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "moduleNameMapper": {
      "^mongoose$": "<rootDir>/../../node_modules/mongoose"
    }
  }
}
